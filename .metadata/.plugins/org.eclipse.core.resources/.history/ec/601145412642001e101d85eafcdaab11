 class NumberConverter
{
    public  String Convert(int number)
    {
        String S1 = new Integer(number).toString();

       for(int i=0;i<=S1.length();i++)
       {
    	Char S2 = S1.charAt(i)  
    	   
       }
   return S1;
        
    }

}
